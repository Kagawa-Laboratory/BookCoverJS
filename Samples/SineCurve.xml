<xml xmlns="http://www.w3.org/1999/xhtml">
  <block type="bookcover_card_frame">
    <value name="PAPER_SPEC">
      <block type="bookcover_cardspec">
        <field name="CARDSPEC">エーワン F8A4-5</field>
      </block>
    </value>
    <statement name="DO">
      <block type="variables_set">
        <field name="VAR">回転数</field>
        <value name="VALUE">
          <block type="math_number">
            <field name="NUM">5</field>
          </block>
        </value>
        <next>
          <block type="variables_set">
            <field name="VAR">回転角度</field>
            <value name="VALUE">
              <block type="math_arithmetic">
                <field name="OP">DIVIDE</field>
                <value name="A">
                  <block type="math_number">
                    <field name="NUM">360</field>
                  </block>
                </value>
                <value name="B">
                  <block type="variables_get">
                    <field name="VAR">回転数</field>
                  </block>
                </value>
              </block>
            </value>
            <next>
              <block type="bookcover_foreachcard">
                <field name="CARD">カード</field>
                <field name="COUNTER">カウンター</field>
                <field name="CLIP">TRUE</field>
                <field name="FRAME">TRUE</field>
                <field name="MARGIN">1</field>
                <statement name="DO">
                  <block type="bookcover_translate">
                    <value name="x">
                      <block type="math_arithmetic">
                        <field name="OP">DIVIDE</field>
                        <value name="A">
                          <block type="bookcover_cardWidth">
                            <value name="CARD">
                              <block type="variables_get">
                                <field name="VAR">カード</field>
                              </block>
                            </value>
                          </block>
                        </value>
                        <value name="B">
                          <block type="math_number">
                            <field name="NUM">2</field>
                          </block>
                        </value>
                      </block>
                    </value>
                    <value name="y">
                      <block type="math_arithmetic">
                        <field name="OP">DIVIDE</field>
                        <value name="A">
                          <block type="bookcover_cardHeight">
                            <value name="CARD">
                              <block type="variables_get">
                                <field name="VAR">カード</field>
                              </block>
                            </value>
                          </block>
                        </value>
                        <value name="B">
                          <block type="math_number">
                            <field name="NUM">2</field>
                          </block>
                        </value>
                      </block>
                    </value>
                    <next>
                      <block type="bookcover_matrix">
                        <statement name="statements">
                          <block type="controls_repeat_ext">
                            <value name="TIMES">
                              <block type="variables_get">
                                <field name="VAR">回転数</field>
                              </block>
                            </value>
                            <statement name="DO">
                              <block type="bookcover_rotate">
                                <value name="angle">
                                  <block type="variables_get">
                                    <field name="VAR">回転角度</field>
                                  </block>
                                </value>
                                <next>
                                  <block type="bookcover_stroke">
                                    <value name="COLOUR">
                                      <block type="bookcover_rotate_h">
                                        <value name="colour">
                                          <block type="bookcover_colour">
                                            <field name="COLOUR">#ff6666</field>
                                          </block>
                                        </value>
                                        <value name="angle">
                                          <block type="math_arithmetic">
                                            <field name="OP">MULTIPLY</field>
                                            <value name="A">
                                              <block type="bookcover_angle">
                                                <field name="angle">105</field>
                                              </block>
                                            </value>
                                            <value name="B">
                                              <block type="variables_get">
                                                <field name="VAR">カウンター</field>
                                              </block>
                                            </value>
                                          </block>
                                        </value>
                                      </block>
                                    </value>
                                    <next>
                                      <block type="bookcover_stroke_opacity">
                                        <value name="opacity">
                                          <block type="math_number">
                                            <field name="NUM">0.2</field>
                                          </block>
                                        </value>
                                        <next>
                                          <block type="controls_for">
                                            <field name="VAR">i</field>
                                            <value name="FROM">
                                              <block type="math_number">
                                                <field name="NUM">1</field>
                                              </block>
                                            </value>
                                            <value name="TO">
                                              <block type="math_number">
                                                <field name="NUM">50</field>
                                              </block>
                                            </value>
                                            <value name="BY">
                                              <block type="math_number">
                                                <field name="NUM">1</field>
                                              </block>
                                            </value>
                                            <statement name="DO">
                                              <block type="variables_set">
                                                <field name="VAR">サインカーブ</field>
                                                <value name="VALUE">
                                                  <block type="math_arithmetic">
                                                    <field name="OP">MULTIPLY</field>
                                                    <value name="A">
                                                      <block type="math_number">
                                                        <field name="NUM">10</field>
                                                      </block>
                                                    </value>
                                                    <value name="B">
                                                      <block type="math_trig">
                                                        <field name="OP">SIN</field>
                                                        <value name="NUM">
                                                          <block type="math_arithmetic">
                                                            <field name="OP">MULTIPLY</field>
                                                            <value name="A">
                                                              <block type="variables_get">
                                                                <field name="VAR">i</field>
                                                              </block>
                                                            </value>
                                                            <value name="B">
                                                              <block type="bookcover_angle">
                                                                <field name="angle">15</field>
                                                              </block>
                                                            </value>
                                                          </block>
                                                        </value>
                                                      </block>
                                                    </value>
                                                  </block>
                                                </value>
                                                <next>
                                                  <block type="bookcover_line">
                                                    <value name="x1">
                                                      <block type="math_number">
                                                        <field name="NUM">0</field>
                                                      </block>
                                                    </value>
                                                    <value name="y1">
                                                      <block type="math_number">
                                                        <field name="NUM">0</field>
                                                      </block>
                                                    </value>
                                                    <value name="x2">
                                                      <block type="variables_get">
                                                        <field name="VAR">i</field>
                                                      </block>
                                                    </value>
                                                    <value name="y2">
                                                      <block type="variables_get">
                                                        <field name="VAR">サインカーブ</field>
                                                      </block>
                                                    </value>
                                                  </block>
                                                </next>
                                              </block>
                                            </statement>
                                          </block>
                                        </next>
                                      </block>
                                    </next>
                                  </block>
                                </next>
                              </block>
                            </statement>
                          </block>
                        </statement>
                        <next>
                          <block type="bookcover_rotate">
                            <value name="angle">
                              <block type="math_arithmetic">
                                <field name="OP">MULTIPLY</field>
                                <value name="A">
                                  <block type="math_number">
                                    <field name="NUM">-1</field>
                                  </block>
                                </value>
                                <value name="B">
                                  <block type="math_arithmetic">
                                    <field name="OP">MULTIPLY</field>
                                    <value name="A">
                                      <block type="variables_get">
                                        <field name="VAR">回転角度</field>
                                      </block>
                                    </value>
                                    <value name="B">
                                      <block type="variables_get">
                                        <field name="VAR">回転数</field>
                                      </block>
                                    </value>
                                  </block>
                                </value>
                              </block>
                            </value>
                            <next>
                              <block type="bookcover_translate">
                                <value name="x">
                                  <block type="math_arithmetic">
                                    <field name="OP">MULTIPLY</field>
                                    <value name="A">
                                      <block type="math_number">
                                        <field name="NUM">-1</field>
                                      </block>
                                    </value>
                                    <value name="B">
                                      <block type="math_arithmetic">
                                        <field name="OP">DIVIDE</field>
                                        <value name="A">
                                          <block type="bookcover_cardWidth">
                                            <value name="CARD">
                                              <block type="variables_get">
                                                <field name="VAR">カード</field>
                                              </block>
                                            </value>
                                          </block>
                                        </value>
                                        <value name="B">
                                          <block type="math_number">
                                            <field name="NUM">2</field>
                                          </block>
                                        </value>
                                      </block>
                                    </value>
                                  </block>
                                </value>
                                <value name="y">
                                  <block type="math_arithmetic">
                                    <field name="OP">MULTIPLY</field>
                                    <value name="A">
                                      <block type="math_number">
                                        <field name="NUM">-1</field>
                                      </block>
                                    </value>
                                    <value name="B">
                                      <block type="math_arithmetic">
                                        <field name="OP">DIVIDE</field>
                                        <value name="A">
                                          <block type="bookcover_cardHeight">
                                            <value name="CARD">
                                              <block type="variables_get">
                                                <field name="VAR">カード</field>
                                              </block>
                                            </value>
                                          </block>
                                        </value>
                                        <value name="B">
                                          <block type="math_number">
                                            <field name="NUM">2</field>
                                          </block>
                                        </value>
                                      </block>
                                    </value>
                                  </block>
                                </value>
                                <next>
                                  <block type="bookcover_stroke">
                                    <value name="COLOUR">
                                      <block type="bookcover_none"></block>
                                    </value>
                                    <next>
                                      <block type="variables_set">
                                        <field name="VAR">今の色</field>
                                        <value name="VALUE">
                                          <block type="bookcover_rotate_h">
                                            <value name="colour">
                                              <block type="bookcover_colour">
                                                <field name="COLOUR">#660000</field>
                                              </block>
                                            </value>
                                            <value name="angle">
                                              <block type="math_arithmetic">
                                                <field name="OP">MULTIPLY</field>
                                                <value name="A">
                                                  <block type="bookcover_angle">
                                                    <field name="angle">45</field>
                                                  </block>
                                                </value>
                                                <value name="B">
                                                  <block type="variables_get">
                                                    <field name="VAR">カウンター</field>
                                                  </block>
                                                </value>
                                              </block>
                                            </value>
                                          </block>
                                        </value>
                                        <next>
                                          <block type="bookcover_fill">
                                            <value name="COLOUR">
                                              <block type="variables_get">
                                                <field name="VAR">今の色</field>
                                              </block>
                                            </value>
                                            <next>
                                              <block type="procedures_callnoreturn">
                                                <mutation name="文字を書く"></mutation>
                                              </block>
                                            </next>
                                          </block>
                                        </next>
                                      </block>
                                    </next>
                                  </block>
                                </next>
                              </block>
                            </next>
                          </block>
                        </next>
                      </block>
                    </next>
                  </block>
                </statement>
              </block>
            </next>
          </block>
        </next>
      </block>
    </statement>
  </block>
